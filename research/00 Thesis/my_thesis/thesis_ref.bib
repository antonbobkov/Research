@Article{laskowski,
  author   = {Laskowski, Michael C.},
  title    = {A simpler axiomatization of the Shelah-Spencer almost sure theories},
  journal  = {Israel Journal of Mathematics},
  year     = {2007},
  volume   = {161},
  number   = {1},
  pages    = {157--186}, 
  issn     = {1565-8511},
  abstract = {We give an explicit AE-axiomatization of the almost sure theories of sparse random graphs G(n, n                −$\alpha$) of Shelah-Spencer. In the process we give a method of constructing extensions of graphs whose `relative dimension' is negative, but arbitrarily small. We describe the existentially closed and locally finite models of the theory and produce types of dimension zero. We offer a useful characterization of forking and generalize results about stability and the Dimensional Order Property (DOP) that were known for graphs to arbitrary relational languages.},
  doi      = {10.1007/s11856-007-0077-8},
  url      = {http://dx.doi.org/10.1007/s11856-007-0077-8},
}

@Article{reduct,
  author   = {Leenknegt, Eva},
  title    = {Reducts of p-adically closed fields},
  journal  = {Archive for Mathematical Logic},
  year     = {2014},
  volume   = {53},
  number   = {3},
  pages    = {285--306},
  issn     = {1432-0665},
  abstract = {In this paper, we consider reducts of p-adically closed fields. We introduce a notion of shadows: sets                                                                           {\$}{\$}{\{}M{\_}f = {\backslash}{\{}(x,y) {\backslash}in K^2 {\backslash}mid |y| = |f(x)|{\backslash}{\}}{\}}{\$}{\$}                                                                                                              M                          f                                                                          =                          {\{}                                                      (                            x                            ,                            y                            )                                                    ∈                                                                          K                          2                                                ∣                                                  |                          y                          |                                                =                                                  |                          f                                                      (                            x                            )                                                    |                          {\}}                                                                                                     , where f is a semi-algebraic function. Adding symbols for such sets to a reduct of the ring language, we obtain expansions of the semi-affine language where multiplication is nowhere definable, thus giving a negative answer to a question posed by Marker, Peterzil and Pillay. The second main result of this paper is the fact that in p-adic fields, full multiplication becomes definable if we add a rational function to the semi-affine language.},
  doi      = {10.1007/s00153-013-0366-3},
  url      = {http://dx.doi.org/10.1007/s00153-013-0366-3},
}

